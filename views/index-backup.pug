extends layout/layout

block content
    div.container-header
        div.container
            div.header
                div.row
                    div.col-md-4.text-left.d-none.d-md-block.header-logos
                        a(href="#")
                            img.img(src=prefix + "/images/kultur-turizm-bakanligi.png", width='147', height='96')
                        a(href="#")
                            img.img(src=prefix + "/images/baskent-kultur-yolu.svg", width='107', height='66')
                    div.col-md-8
                        div.col
                            ul.nav.d-none.d-md-flex
                                li
                                    a(href='events.html') Etkinlikler
                                li
                                    a(href='#') Mekanlar
                                li
                                    a(href='#') Haberler
                                li
                                    div.header-search-area
                                        div.header-search-area-close.d-none
                                            em.icon.icon-close
                                        em.icon.icon-search.header-search-area-search
                                        input(
                                            type='text'
                                            class='header-search-bar'
                                            placeholder="Etkinlik Ara"
                                        )


                        include layout/header-search-area-categories

                        div.col-md-3.text-center.d-md-none.mb-4
                            img.img(src=prefix + "/images/logo.png", width='168', height='142')
                        div.col
                            p.search-bar-information Ne yapmak istediğinizden emin değil misiniz? Harika!
                        div.col
                            a(href="", class="suggestions") Her Öneriye Açığım


                    div.col.d-none.d-none
                        a(href="#header-menu", class="header-desktop-menu fancy-box-item-header")
                            em.icon.icon-hamburger-menu
        div.container-fluid.search-open.d-none
    div.main.d-none
        div.categories
            div.container
                div.row
                    div.col-lg-3.d-md-none.d-lg-block
                    div.col-lg-6
                        p.categories-title Kategoriler
                        ul.d-flex.justify-content-start
                            li
                                a(href="#")
                                    img.img(src=prefix + "/icons/exhibit.svg", height='28')
                                    span Sergiler
                            li
                                a(href="#")
                                    img.img(src=prefix + "/icons/theatre.svg", height='28')
                                    span Tiyatrolar
                            li
                                a(href="#")
                                    img.img(src=prefix + "/icons/concert.svg", height='28')
                                    span Konserler
                            li
                                a(href="#")
                                    img.img(src=prefix + "/icons/performance.svg", height='28')
                                    span Performanslar
                    div.col-lg-3

        div.all-activities
            div.container
                div.row
                    div.col-12
                        ul.all-activities-tabs.d-flex
                            li
                                button(data-id="today-activities").active Bugün
                            li
                                button(data-id="popular-activities") Popüler
                            li
                                button(data-id="free-activities") Ücretsiz
                    div.col-12
                        div.today-activities.all-activities-flow
