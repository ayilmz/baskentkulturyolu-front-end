        div.mobile-application-support
            div.container
                div.row.justify-content-md-center
                    div.col-12.col-md-4.align-self-center
                        p.mobile-application-support-title Başken Kültür Yolu
                        p.mobile-application-support-sub-title Mobil Uygulaması
                        p.mobile-application-support-description How We Keep Brand Consistency in Our Visual Language — A Design System for Illustrations
                    div.col-6.col-md-auto
                        img.img(src=prefix + "/images/mobile-application.png", width='175', height='302')
                    div.col-6.col-md-4.align-self-center
                        ul.justify-content-md-center.mobile-application-support-images
                            li.float-left
                                a(href="#")
                                    img.img(src=prefix + "/images/google-play.png", width='135', height='40')
                            li.float-left
                                a(href="#")
                                    img.img(src=prefix + "/images/app-store.png", width='135', height='40')

        div.sponsorship
            div.container
                div.row
                    div.col
                        p.sponsorship-title Sponsorlar
                div.row.sponsorship-slider
                    div.col
                        ul.d-flex.justify-content-between
                            li
                                img.img(src=prefix + "/images/sponsor-eczacibasi.png", title="Eczacıbaşı")
                            li
                                img.img(src=prefix + "/images/sponsor-vitra.png", title="Vitra")
                            li
                                img.img(src=prefix + "/images/sponsor-basf.png", title="Basf")
                            li
                                img.img(src=prefix + "/images/sponsor-zorlu.png", title="Zorlu")


    include layout/footer

    include layout/header-menu

