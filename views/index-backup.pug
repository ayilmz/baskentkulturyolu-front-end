                div.row.d-flex.justify-content-center.align-items-center
                    button.all-activities-show-more.d-flex.justify-content-center.align-items-center
                        img.img(src=prefix + "/images/plus.svg", width='17', height='17')
                        span Daha fazla göster
        div.container-instagram
            div.container
                div.row
                    div.col-md-6.col-lg-4.instagram-information.d-flex
                        img.img(src=prefix + "/images/instagram-avatar.png", width='54', height='54')
                        p
                            span.instagram-information-title @BaskentKulturYolu
                            span.instagram-information-counts <b>240 Post</b> 381K Takipçi

                    div.col-md-3.col-lg-4.instagram-information.d-flex.justify-content-center.justify-content-md-start
                        a(href="#", class="d-flex justify-content-center align-items-center") Takip Et
                    div.col-md-3.col-lg-4.d-none.d-md-flex.justify-content-center.justify-content-md-start
                        button.slider-nav.instagram-slider-prev-button
                            em.icon.icon-prev
                        button.slider-nav.instagram-slider-next-button
                            em.icon.icon-next
        div.container-fluid
            div.row.instagram-flow
                div.col.swiper.mySwiper
                    div.instagram-flow-slider.swiper-wrapper
                        div.slider.swiper-slide
                            div.instagram-flow-slider-item
                                img.instagram-flow-image(src=prefix + "/images/instagram-image-1.png", width='300', height='263')
                                div.instagram-flow-account-name
                                    img.img(src=prefix + "/images/instagram-avatar.png", width='32', height='32')
                                    span @BaskentKulturYolu
                                p How We Keep Brand Consistency in Our Visual Language — A Design System for Illustrations

                        div.slider.swiper-slide
                            div.instagram-flow-slider-item
                                img.instagram-flow-image(src=prefix + "/images/instagram-image-2.png", width='300', height='263')
                                div.instagram-flow-account-name
